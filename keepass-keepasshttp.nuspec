<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>keepass-keepasshttp</id>
    <title>KeePassHttp</title>
    <version>1.8.4.220170623</version>
    <packageSourceUrl>https://github.com/kendaleiv/chocolatey-keepass-keepasshttp</packageSourceUrl>    
    <authors>pfn</authors>
