    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <docsUrl>https://github.com/pfn/keepasshttp/blob/master/README.md</docsUrl>
    <bugTrackerUrl>https://github.com/pfn/keepasshttp/issues</bugTrackerUrl>
    <dependencies>
      <dependency id="keepass" version="[2.0,3.0)" />
    </dependencies>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
